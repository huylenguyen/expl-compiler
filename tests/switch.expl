// Input a number between 1 and 3 and get the input * 2. Default returns 0

let v : int = exec(0)

function exec (x : int) : int{
    let x : int = 0
  	let a : int = 1
  	let b : int = 2
  	let c : int = 0

    input x
    
    switch x {
        case 1 {
        	output a
        }
        case 2 {
        	output b
        }
        default {
        	output c
        }
    }
    return x
}
